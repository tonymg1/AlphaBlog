<h1 class="text-center mt-4">Edit category name</h1>

<div class="row justify-content-center">
  <div class="col-10">
    <%= render 'shared/errors', obj: @category %>
    
    <%= render 'form' %>
    
    <div class="text-center mb-4">
      <%= link_to '[Cancel and return to category]', category_path(@category), class:"text-info text-decoration-none"%>
    </div>
  </div>
</div>
